{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "experimentalDecorators": true,
    "baseUrl": "./",
    "emitDecoratorMetadata": true,
    "strict": true,
    "paths": {
      "~components/*": ["src/components/*"],
      "~screens/*": ["src/screens/*"],
      "~assets/*": ["assets/*"],
      "~constants/*": ["constants/*"]
    },
    "strictPropertyInitialization": false
  }
}
